                l1 = true;
                
            }
        
        ///2nd set
        }else if leaves == 2{
            if l2 == false{
                pat = 'f';
                alarm[2] = 5 * room_speed;              
                l2 = true;
                
            }
            
            
            if(s_x mod 5*mult == 0){
                instance_create(x,y,obj_CH_leaf);
            }
                        
        ///3rd set and next phase
        }else if leaves == 3{
            if l3 == false{
                alarm[3] = 0.6 * room_speed;
                l3 = true;
                
            }
        }
        
    break;
    
    //Grab the player
    case 4:
        r_vis = false;
        r_solid = false;
        restart = true;
        
        if not instance_exists(obj_CH_groots) and bene == true{
            instance_create(obj_body.x,obj_body.y,obj_CH_groots);
        }
        
    break;
}
