    ss_x = -1;
    mult = irandom_range(2,3);
}
if(s_x &lt; 180 and ss_x == -1){
    ss_x = 1;
    mult = irandom_range(2,3);
}

phy_position_x = i_x;
phy_position_y = i_y;

if keyboard_check(vk_alt){
    start = true;
}

///Determine action state
if start == true{
    if once == false{
        
        once = true;
    }
    
    if bene == true{    
        if state == 0{
            //Swing down
            state = 1;
        }
        
        if state == 4{
            grab = true;
        }
        
    }else if bene == false{
        image_speed = 0.3;
        
        if(image_index &gt;= 15){
            instance_create(x,y,obj_CH_deer_state);
            while instance_exists(obj_CH_root){
                with obj_CH_root{
                    instance_destroy();
                }
            }
            instance_destroy();
        }
    }
    
}else {
    state = 0;
    
}

if(b_alarm_start == true){
    alarm[0] = 1 * room_speed;
}

///Action state based changes
switch(state){
    //Dormant state
    case 0:
        global.b_spd = 0;
        image_speed = 0;
        
    break;
    
    //Root attack
