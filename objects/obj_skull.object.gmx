            <string>///Exist
if(place_meeting(x,y,obj_ppon)){
    point_direction(obj_body.x,obj_body.y,x,y)
    visible = true;
    depth = -y;
    
    if(trigger == false){
        global.combat += 1;
        trigger = true;
    }

    /// Enemy States
    if dis &gt; 100 {
        state = 0;
        
    }else if dis &lt;= 100{
        state = 1;

    }
    
    ///Enemy Behaevior
    switch(state){
        case 0:  //Flames
            
        break;
        case 1:  //Run away
            
            //Get hspd and vspd
            hspd = lengthdir_x(e_spd,dir);
            vspd = lengthdir_y(e_spd,dir);
   
            //Move
            phy_position_x += hspd;
            phy_position_y += vspd;
            
        break;
   }
    
    image_blend = c_white;
    ///Get Damaged
    if(place_meeting(x,y,obj_swing)){
        e_hp -= global.p_atk div e_def;
        alarm[4] = 20;
        if flash == false{
            flash = true;
            image_blend = c_red;
        }
    }
        
    ///Die
    if(e_hp &lt;= 0){
        global.combat -= 1;
        instance_destroy();
        if((irandom(99) + 1) &lt;= 10){
                instance_create(x,y,obj_hheart);
                
            }else if((irandom(99) + 1) == 1){
                instance_create(x,y,obj_fheart);
                
            }
    }

    if(place_meeting(x,y,obj_sword_t) and obj_sword_t.image_speed&gt;0){
        e_hp -= global.p_satk div e_sdef;
    }
    
}
