        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Exist
if place_meeting(x,y,obj_ppon) and k == false{
    image_speed = 5;
    alarm[1] = 10;
    k = true;
    
    if(trigger == false){
        global.combat += 1;
        trigger = true;
    }
}

if place_meeting(x,y,obj_ppon) and go == true{
    visible = true;
    depth = -y;
    
    /// Enemy States
    if (state == 3 or state == 1 or state == 2){
        sprite_index = spr_skull_o;
        if dis &lt; 185{
            state = 1;

        }else if dis &gt; 215{
            state = 2;
        }else {
            state = 3;
        }
    }
    
    ///Enemy Behaevior
    switch(state){
        case 0:  //Flames
            sprite_index = spr_skull_f;
            
            off_x = 0;
            off_y = 0;
            
            if fl_dir == 'card' and casting == false{
                instance_create(x+16,y,obj_skull_fl);
                instance_create(x,y+16,obj_skull_fl);
                instance_create(x-16,y,obj_skull_fl);
                instance_create(x,y-16,obj_skull_fl);
                
                fl_dir = 'diag';
                casting = true;
                alarm[2] = 1 * room_speed;
                
            }else if fl_dir == 'diag' and casting == false{
                instance_create(x+12,y+12,obj_skull_fl);
                instance_create(x+12,y-12,obj_skull_fl);
                instance_create(x-12,y+12,obj_skull_fl);
                instance_create(x-12,y-12,obj_skull_fl);
                
                fl_dir = 'card';
                casting = true;
                alarm[2] = 1 * room_speed;
            }
            
            hspd = 0;
            vspd = 0;
            
        break;
        case 2:  //Run away
            //Get hspd and vspd
            dir = point_direction(x,y,obj_body.x,obj_body.y);
            hspd = lengthdir_x(e_spd,dir);
            vspd = lengthdir_y(e_spd,dir);
            
            off_x = obj_body.hspd;
            off_y = obj_body.vspd;
            
        break;
        case 1:  //Get closer   
            //Get hspd and vspd
            dir = point_direction(obj_body.x,obj_body.y,x,y);
            hspd = lengthdir_x(e_spd,dir);
            vspd = lengthdir_y(e_spd,dir);
            
            off_x = obj_body.hspd;
            off_y = obj_body.vspd;

        break;
        case 3:
            //do nothing
            hspd = 0;
            vspd = 0;
            
            off_x = 0;
            off_y = 0;
            
            if casting == false{
                state = 0;
            }
        break;
    }
    
    phy_position_x += hspd - off_x;
    phy_position_y += vspd - off_y;
    
    ///Get Damaged
    image_blend = c_white;
    if(place_meeting(x,y,obj_swing)){
        e_hp -= global.p_atk div e_def;
        damaged = true;
    }
    
    if(place_meeting(x,y,obj_sword_t) and obj_sword_t.image_speed&gt;0){
        e_hp -= global.p_satk div e_sdef;
        damaged = true;
    }
    
    if damaged == true{
        alarm[4] = 20;
        if flash == false{
            flash = true;
            image_blend = c_red;
        }
    }
    
    ///Die
    if(e_hp &lt;= 0){
        global.combat -= 1;
        instance_destroy();
        if((irandom(99) + 1) &lt;= 10){
            instance_create(x,y,obj_hheart);
                
        }else if((irandom(99) + 1) == 1){
            instance_create(x,y,obj_fheart);
                
        }
    }
    
    go = true;
}
